{
  "name": "cryptopress",
  "version": "1.0.2",
  "description": "create an express app in one shot ! intgerated with Typescript , Prettier , Eslint , Jasmine",
  "main": "./build/index.js",
  "scripts": {
    "dev": "nodemon ./build/index.js",
    "start": "npx rimraf build && npx tsc && node ./build/index.js",
    "clean": "npx rimraf build",
    "build": "npx rimraf build && npx tsc",
    "test": "npx rimraf build && npx tsc && jasmine",
    "format": "npx prettier --write 'src/**/*.{ts,js}' ",
    "lint": "npx eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crypt00o/cryptopress.git"
  },
  "keywords": [
    "cryptopress",
    "create express app",
    "express",
    "express app",
    "CryptoPress",
    "Cryptopress"
  ],
  "author": "Eslam Mohamed Moawed Elabd",
  "license": "MIT",
  "bin": {
    "cryptopress": "./build/index.js"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
